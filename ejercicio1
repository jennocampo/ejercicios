import random

print("**** Ciencias de la Computacion I - 2024")
print("**** Laboratorio 01 - Ejercicio 1 ****")  #error sintactico en print 
print()
nombre = input("Ingrese su nombre: ")
apellido = input("Ingrese su apellido: ")
edad = int(input("Ingrese su edad:")) #se agrego el int 
x = len(nombre)
y = len(apellido) #apellido esta mal escrito 
respuesta = (x * y * (random.randint(0, edad + 2) +  random.randint(0, edad + 10))) #random estaba mal escrito
print("Felicitaciones " + nombre + "!!!")
print("Ha ejecutado su primer programa en Python!")
print("Su respuesta para este programa es: ", respuesta)
print() #faltaba un parentesis 
